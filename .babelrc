{
	"comments": false,
	"env": {
		"production": {
			"presets": [
				"minify"
			]
		}
	},
	"presets": [
		"@babel/preset-env",
		"@babel/preset-typescript"
	],
	"plugins": [
		[
			"module-resolver",
			{
				"root": [
					"./src"
				],
				"alias": {},
				"extensions": [
					".js",
					".jsx",
					".es",
					".es6",
					".mjs",
					".ts",
					".tsx"
				]
			}
		]
	],
	"ignore": [
		"node_modules",
		"dist",
		"**/*.d.ts",
		"**/__tests__/*"
	]
}